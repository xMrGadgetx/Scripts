-- Scripts:
function SCRIPT_POWR72_FAKESCRIPT() -- OpenMenu.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = OpenMenu
	local Frame = script.Parent.Parent.MainFrame
	local button = script.Parent
	Frame.Visible = false
	
	button.MouseButton1Click:Connect(function()
		if button.Text == "Open" then 
			Frame.Visible = true
			button.Text = "Close"
			button:TweenPosition(UDim2.new(0, 0.75, 0.75, 0),"Out","Quint",3,true)
		elseif button.Text == "Close" then
			Frame.Visible = false
			button.Text = "Open"
			button:TweenPosition(UDim2.new(0, -0.001, -0.001, 0),"Out","Quint",3,true)
		end 
	end)

end
coroutine.resume(coroutine.create(SCRIPT_POWR72_FAKESCRIPT))
function SCRIPT_GMWC76_FAKESCRIPT() -- Speed.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Speed
	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/xMrGadgetx/Scripts/master/FERRYLT2", true))()
	end)

end
coroutine.resume(coroutine.create(SCRIPT_GMWC76_FAKESCRIPT))
function SCRIPT_DCBM86_FAKESCRIPT() -- Back.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Back
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.LoginFrame.Visible = true
	end)

end
coroutine.resume(coroutine.create(SCRIPT_DCBM86_FAKESCRIPT))
